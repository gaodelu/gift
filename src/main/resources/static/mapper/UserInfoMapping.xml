<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mochi.springboot.gift.cust.UserInfoMapper">

    <select id="findByUserName" resultType="com.mochi.springboot.gift.cust.UserInfo">
        select * from L_USER where username = #{userName}
    </select>

</mapper>